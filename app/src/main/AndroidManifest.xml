        <activity android:name=".Activitys.BankActivity" />
        <activity android:name=".Activitys.ShopActivity" />
        <activity android:name=".Activitys.DealerActivity" />
        <activity android:name=".Activitys.HospitalActivity" />
        <activity android:name=".Activitys.QuestActivity" />
        <activity android:name=".Activitys.CasinoActivity" />
        <activity android:name=".Activitys.PrisonActivity" />
        <activity android:name=".Activitys.VIPActivity" />
        <activity android:name=".Activitys.WhoresActivity" />
        <activity android:name=".Activitys.HscoreActivity" />
        <activity android:name=".Activitys.AchieveActivity" />
